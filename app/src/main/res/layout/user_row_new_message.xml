<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/new_message_layout_contact_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/new_message_layout_contact_picture"
        app:layout_constraintStart_toEndOf="@+id/new_message_layout_contact_picture"
        app:layout_constraintTop_toTopOf="@+id/new_message_layout_contact_picture"
        app:layout_constraintVertical_bias="0.169" />

    <ImageView
        android:id="@+id/new_message_layout_contact_picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/new_message_layout_contact_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/new_message_layout_contact_name"
        app:layout_constraintStart_toStartOf="@+id/new_message_layout_contact_name"
        app:layout_constraintTop_toTopOf="@+id/new_message_layout_contact_name" />
</androidx.constraintlayout.widget.ConstraintLayout>